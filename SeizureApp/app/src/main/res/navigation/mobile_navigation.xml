<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.seizureapp.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_healthMetrics"
        android:name="com.example.seizureapp.ui.healthmetrics.HealthMetricsFragment"
        android:label="@string/title_healthMetrics"
        tools:layout="@layout/fragment_healthmetrics" />

    <fragment
        android:id="@+id/navigation_connections"
        android:name="com.example.seizureapp.ui.connections.ConnectionsFragment"
        android:label="@string/title_connections"
        tools:layout="@layout/fragment_connections" />

    <fragment
        android:id="@+id/navigation_testing"
        android:name="com.example.seizureapp.ui.testing.TestingFragment"
        android:label="Testing"
        tools:layout="@layout/fragment_testing"/>
</navigation>