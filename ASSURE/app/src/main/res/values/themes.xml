<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ASSURE" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#FF9B4B</item>
        <item name="colorPrimaryVariant">#7E4D25</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#FF9B4B</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.alt" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#5C7F6D</item>
        <item name="colorPrimaryVariant">#374C41</item>
        <item name="colorOnPrimary">#F4F1DE</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#81B29A</item>
        <item name="colorSecondaryVariant">#81B29A</item>
        <item name="colorOnSecondary">#F4F1DE</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:windowBackground">#F4F1DE</item>
    </style>
</resources>